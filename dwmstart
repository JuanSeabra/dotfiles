## my start up script dwmstart

#!/bin/bash

exec dwmstatusbar

function run {
  if ! pgrep -f $1 ;
  then
    $@&
  fi
}
run light-locker --lock-on-suspend 
run picom 
run nitrogen --restore 
# run insync start 
# run dropbox start 
run dunst 
run nm-applet 
run blueman-applet 
run light-locker --lock-on-suspend 
run caffeine 

run dwm

