## my start up script dwmstart

#!/bin/bash
/home/waldomiro/dotfiles/dwmstatusbar

exec light-locker --lock-on-suspend &
exec picom &
exec nitrogen --restore &
# exec insync start &
# exec dropbox start &
exec dunst &
exec nm-applet &
exec blueman-applet &
exec light-locker --lock-on-suspend &
exec caffeine &

exec dwm

